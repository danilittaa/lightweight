<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Title</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
  </head>
  <body
    style="
      display: flex-col;
      justify-content: center;
      background-color: rgb(186, 211, 220);
      padding: 60px 60px 0;
    "
  >
    <h2>
      The most popular breed:
      <span style="color: rgb(97, 95, 95)"><%= breed %></span>
    </h2>
    <table style="margin-top: 60px" class="table table-striped">
      <thead>
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Age</th>
          <th>Breed</th>
        </tr>
      </thead>
      <tbody>
        <% cats.forEach(function(item) { %>
        <tr>
          <td><%= item.id %></td>
          <td><%= item.name %></td>
          <td><%= item.age %></td>
          <td><%= item.breed %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
